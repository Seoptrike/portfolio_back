<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.backend.mapper.ProjectItemMapper">
    <select id="findProjectDetail" resultType="com.backend.domain.ProjectItemsVO">
        SELECT *
        FROM project_items
        WHERE project_id = #{project_id}
    </select>
</mapper>
